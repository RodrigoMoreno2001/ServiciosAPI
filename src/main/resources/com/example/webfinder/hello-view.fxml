<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<VBox alignment="CENTER" prefHeight="720" prefWidth="1240" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.webfinder.HelloController">

    <stylesheets>
        <URL value ="@/Styles.css" />
    </stylesheets>

   <children>
      <AnchorPane prefHeight="720" prefWidth="1240">
          <children>
              <AnchorPane prefHeight="720" prefWidth="1240" id="mainAnchorPane">
                  <children>
                      <TableView fx:id="tabla" layoutX="118.0" layoutY="133.0" prefHeight="200.0" prefWidth="1023.0" id="tablaView">
                          <columns>
                              <TableColumn fx:id="columnaPuerto" prefWidth="75.0" text="Puerto" id="columnaPuerto" />
                              <TableColumn fx:id="columnaTitulo" prefWidth="75.0" text="Titulo" id="columnaTitulo" />
                              <TableColumn fx:id="columnaMotor" prefWidth="75.0" text="Motor" id="columnaMotor" />
                              <TableColumn fx:id="columnaServidor" prefWidth="75.0" text="Servidor" id="columnaServidor" />
                              <TableColumn fx:id="columnaCodigo" prefWidth="75.0" text="Codigo" id="columnaCodigo" />
                          </columns>
                      </TableView>
                      <Button onAction="#onBuscarClick" layoutX="652.0" layoutY="426.0" mnemonicParsing="false" text="Buscar" id="buscarButton" />
                      <TextField fx:id="buscarText" layoutX="481.0" layoutY="426.0" id="buscarTextField" />
                  </children>
              </AnchorPane>
          </children>
      </AnchorPane>
   </children>
</VBox>
